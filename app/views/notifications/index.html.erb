<p id="notice"><%= notice %></p>

<table>
  <tbody>
    <% @notifications.each do |n| %>
      <tr>
        <td><%= n.created_at %></td>
        <td><%= link_to n.text, "/notifications/#{n.id}" %></td>
        <td><%= link_to "Mark read", "/notifications/#{n.id}", method: :delete %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
