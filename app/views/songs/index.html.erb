<table>
    <thead>
    <tr>
        <th>Song</th>
        <th>Artist</th>
        <th>Album</th>
        <th>Duration</th>
        <th></th>
    </tr>
    </thead>
    <% @songs.each do |song| %>
    <tbody>
    <tr>
        <td> <%= song.title %> </td>
        <td> <%= song.artist %> </td>
        <td> <%= song.album %> </td>
        <td> <%= song.duration %> </td>
        <td>
            <% if logged_in? and current_user.can_add_to_queue %>
                <form action=<%="/stations/#{current_user.station.id}"%> method="post" class="in_place_form">
                    <input type="hidden" name="song_id" value=<%= song.id %> >
                    <button class="button button6" type="submit"><i class="fa fa-plus"></i></button>
                </form>
            <% end %>
        </td>
    </tr>
    </tbody>
    <% end %>
</table>
